{
  "buildCommand": "npm run build",
  "outputDirectory": "apps/platform/dist",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://breederhq-api.onrender.com/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
