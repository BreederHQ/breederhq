{
  "name": "@bhq/ui",
  "version": "0.1.0",
  "files": [
    "dist",
    "styles"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./overlay": "./dist/overlay.js",
    "./styles/*": "./styles/*"
  },
  "sideEffects": [
    "*.css",
    "styles/*.css"
  ],
  "dependencies": {
    "@fullcalendar/daygrid": "^6.1.19",
    "@fullcalendar/interaction": "^6.1.19",
    "@fullcalendar/react": "^6.1.19",
    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0",
    "react": ">=19",
    "react-day-picker": "^9.13.0",
    "react-dom": ">=19",
    "zod": "^4.1.12"
  },
  "peerDependencies": {
    "react": ">=19",
    "react-dom": ">=19"
  },
  "devDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.0"
  },
  "tsup": {
    "entry": {
      "index": "src/index.ts",
      "overlay": "src/overlay/index.ts"
    },
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true,
    "sourcemap": true,
    "clean": true,
    "splitting": false,
    "minify": false,
    "external": [
      "react",
      "react-dom",
      "@fullcalendar/react",
      "@fullcalendar/daygrid",
      "@fullcalendar/interaction"
    ]
  },
  "scripts": {
    "build": "tsup"
  }
}
