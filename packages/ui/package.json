{
  "name": "@bhq/ui",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
"exports": {
  ".": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs"
  },
  "./overlay": {
    "types": "./dist/overlay.d.ts",
    "import": "./dist/overlay.mjs",
    "require": "./dist/overlay.cjs"
  },
  "./styles/*": "./styles/*"
},
"files": ["dist", "styles", "bhq.css"],
"sideEffects": ["*.css", "./styles/*"],
"scripts": {
  "build": "tsup",
  "dev": "tsup --watch"
},

  "dependencies": {
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "tsup": "^8.5.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.0"
  }
}
